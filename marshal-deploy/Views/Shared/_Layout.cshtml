                *@
        </div>
        <!-- /main content -->  
        
    </div>
    <!-- /page content -->

    

    @Scripts.Render("~/bundles/jquery") @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>